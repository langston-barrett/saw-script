enable_deprecated;
let f_spec = do {
    llvm_ptr "x" (llvm_int 32);
    llvm_assert_null "x";
    llvm_return {{ 1 : [32] }};
    llvm_verify_tactic abc;
};

let f_spec2 = do {
    llvm_ptr "x" (llvm_int 32);
    llvm_return {{ 0 : [32] }};
    llvm_verify_tactic abc;
};

m <- llvm_load_module "assert-null.bc";
llvm_verify m "f" [] f_spec;
llvm_verify m "f" [] f_spec2;
